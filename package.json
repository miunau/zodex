{
  "name": "@auth70/zodex-esm",
  "version": "0.7.1",
  "type": "module",
  "description": "Type-safe (de)serialization for Zod (esm fork of zodex with minimal deps)",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
      "*": {
          ".": [
              "./dist"
          ]
      }
  },
  "keywords": [
    "zod",
    "serialization",
    "deserialization",
    "reflection"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublish": "pnpm run build"
  },
  "peerDependencies": {
    "zod": "^3.x"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "type-fest": "^4.15.0",
    "typescript": "5.4.4"
  }
}
